<h1>Items</h1>
<hr>
<a href="/createitem">Add new item</a>
<table border="1">
    <tr>
        <th>Name</th>
        <th>Description</th>
        <th>Price</th>
        <th>Stock</th>
        <th>Category</th>
    </tr>
    {{#each items }}
    <tr>
        <td>{{ this.name }}</td>
        <td>{{ this.description }}</td>
        <td>{{ this.price }}</td>
        <td>{{ this.stock }}</td>
        <td>{{ this.category }}</td>
    </tr>
    {{/each}}
</table>